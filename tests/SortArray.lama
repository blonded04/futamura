fun bubbleSort (l) {
    var n = length(l);
    for var i = 0; , i < n,  i := i + 1 do
        for var j = 0; ,  j < n - 1, j := j + 1 do 
            if l[j] > l[j + 1] then 
                var t = l[j];
                l[j] := l[j + 1];
                l[j + 1] := t
            fi  
        od
    od
}

fun generate (l) {
    var n = length(l);
    for var i = 0; ,  i < n,  i := i + 1 do
        l[i] := n - i
    od
}

fun print (l) {
    var n = length(l);
    for var i = 0; , i < n,  i := i + 1 do
        write(l[i])
    od
}

